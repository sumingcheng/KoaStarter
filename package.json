{
  "name": "koastarter",
  "version": "1.0.0",
  "description": "KoaStarter",
  "main": "index.js",
  "scripts": {
    "dev": "redis-server & cross-env NODE_ENV=dev tsx watch app.ts",
    "dev:sit": "redis-server & cross-env NODE_ENV=sit tsx watch app.ts",
    "dev:prod": "redis-server & cross-env NODE_ENV=prod tsx watch app.ts",
    "build:sit": "cross-env NODE_ENV=sit tsup",
    "build:prod": "cross-env NODE_ENV=prod tsup",
    "start:sit": "cross-env NODE_ENV=sit tsx ./dist/app.js",
    "start:prod": "cross-env NODE_ENV=prod tsx ./dist/app.js",
    "watch-server": "pm2 start npm --name 'koaStarter' --watch -- run start",
    "pm2:sit": "pm2 start npm --name 'koaStarter' --log-date-format 'YYYY-MM-DD HH:mm:ss' -- run start",
    "pm2:prod": "pm2 start npm --name 'koaStarter' --log-date-format 'YYYY-MM-DD HH:mm:ss' -- run start",
    "backup": "pm2 start tsx --name 'backup' -- ./bin/backup.ts",
    "restore": "pm2 start tsx --name 'restore' -- ./bin/restore.ts"
  },
  "keywords": [],
  "author": "sumingcheng <cjwdyzdyplxjg@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumingcheng/koastarter.git"
  },
  "bugs": {
    "url": "https://github.com/sumingcheng/koastarter/issues"
  },
  "homepage": "https://github.com/sumingcheng/koastarter#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "husky": "^8.0.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "mockjs": "^1.1.0",
    "mysql2": "^3.6.1",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "sequelize": "^6.33.0",
    "urlencode": "^1.1.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/koa": "^2.13.9",
    "@types/koa-json": "^2.0.21",
    "@types/koa-logger": "^3.1.3",
    "@types/koa-router": "^7.4.5",
    "@types/koa2-cors": "^2.0.3",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.7.1",
    "@types/node-fetch": "^2.6.6",
    "@types/node-schedule": "^2.1.5",
    "@types/sequelize": "^4.28.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "tsup": "^7.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
