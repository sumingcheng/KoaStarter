{
  "name": "koastarter",
  "version": "1.0.0",
  "description": "KoaStarter",
  "main": "index.js",
  "scripts": {
    "dev": "redis-server & cross-env NODE_ENV=dev tsx watch app.ts",
    "build": "cross-env NODE_ENV=prd tsup",
    "start": "cross-env NODE_ENV=prd tsx ./dist/app.js",
    "watch-server": "pm2 start npm --name 'ai-be' --watch -- run start",
    "pm2": "pm2 start npm --name 'ai-be' --log-date-format 'YYYY-MM-DD HH:mm:ss' -- run start",
    "backup": "pm2 start ./bin/backup.js --name 'backup'",
    "restore": "pm2 start ./bin/restore.js --name 'restore'"
  },
  "keywords": [],
  "author": "sumingcheng <cjwdyzdyplxjg@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumingcheng/koastarter.git"
  },
  "bugs": {
    "url": "https://github.com/sumingcheng/koastarter/issues"
  },
  "homepage": "https://github.com/sumingcheng/koastarter#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "husky": "^8.0.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "mockjs": "^1.1.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.1",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "sequelize": "^6.33.0",
    "urlencode": "^1.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/koa": "^2.13.9",
    "@types/koa-json": "^2.0.21",
    "@types/koa-logger": "^3.1.3",
    "@types/koa-router": "^7.4.5",
    "@types/koa2-cors": "^2.0.3",
    "@types/mockjs": "^1.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.7.1",
    "@types/node-fetch": "^2.6.6",
    "@types/sequelize": "^4.28.16",
    "@types/uuid": "^9.0.7",
    "tsup": "^7.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "yaml": "^2.3.2"
  }
}
